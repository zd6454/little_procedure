/* pages/moredetail/moredetail.wxss */
@import '/weui-miniprogram/weui-wxss/dist/style/weui.wxss';
page,view{
  -moz-box-sizing: border-box;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
}
page{
  width: 100%;
  height: 100%;
  padding: 0;
  margin: 0;
  background-color: #ccc;
}
.list{
  width: 100%;
  height: 100%;
  float: left;
}
.record{
  width: 100%;
  overflow: hidden;
  float: left;
  background-color: #efefef;
}
.left,.right{
  width: 50%;
  float: left;
}
.botttt{
  width: 100%;
  float: left;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  background-color: #fff;
}
.imgsss{
  width: 100%;
  height: calc(100% - 60rpx);
  float: left;
}
.left .record_o{
  float: left;
  margin-left: 6%;
}
.right .record_o{
  float: right;
  margin-right: 6%;
}
.record_o{
  width: 92%;
  height: calc(100% - 15rpx);
  overflow: hidden;
  border-radius: 10rpx;
}
.tti{
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  overflow: hidden;
  background-color: #fff;
  font-size: 24rpx;
  padding: 0 5%;
  float: left;
}