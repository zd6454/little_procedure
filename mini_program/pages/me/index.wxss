.menu_box{ width: 100%; background-color: #fff;}
.menu_box.menu_body_top{ overflow: hidden; position: fixed; left: 0; top: 0; z-index: 999;}
 
.menu_body{ width: auto; padding:22rpx 0; margin: 0 auto; position: relative;
			display: flex; justify-content: flex-start; align-items: center;
}
.menu_body:before{ content: ""; width: 100%; min-width: 750rpx; height: 2rpx; background-color: #ddd; position: absolute; left: 50%; bottom: 0;
				-webkit-transform: translateX(-50%);
						transform: translateX(-50%);
}
.menu_body:after{ content: ""; width:187.5rpx; height: 3rpx; background-color: #fc2549; position: absolute; left: 0; bottom: 0;
				-webkit-transition:all linear .3s;
						transition:all linear .3s;
}
.menu_li{ width: 187.5rpx; height: 52rpx; line-height: 52rpx; text-align: center; color: #000; font-size: 34rpx; position: relative;
				-webkit-transition:all linear .3s;
						transition:all linear .3s;
}
.menu_li:not(:first-child):after{ content: ""; width: 2rpx; height: 100%; background-color: #ddd; position: absolute; left: 0; top: 0;}
 
.menu_body.menu_body_cur0 .menu_li:nth-child(1) ,
.menu_body.menu_body_cur1 .menu_li:nth-child(2) ,
.menu_body.menu_body_cur2 .menu_li:nth-child(3) ,
.menu_body.menu_body_cur3 .menu_li:nth-child(4) { color: #fc2549; font-weight: bold;}
.menu_body.menu_body_cur0:after{ left:0;}
.menu_body.menu_body_cur1:after{ left: 187.5rpx;}
.menu_body.menu_body_cur2:after{ left: calc(187.5rpx * 2);}
.menu_body.menu_body_cur3:after{ left: calc(187.5rpx * 3);}

.navigator-box {
  opacity: 0;
  position: relative;
  background-color: #fff;
  line-height: 66rpx;
  font-size: 26rpx;
  transform: translateY(-50%);
  transition: 0.3s;
}

.navigator-box-show {
  opacity: 1;
  transform: translateY(0);
}

.navigator {
  padding: 20rpx 30rpx;
  position: relative;
  display: flex;
  align-items: center;
}

.navigator:before {
  content: " ";
  position: absolute;
  left: 30rpx;
  top: 0;
  right: 30rpx;
  height: 2rpx;
  border-top: 1rpx solid #d8d8d8;
  color: #d8d8d8;
}

.navigator:first-child:before {
  display: none;
}

.navigator-text {
  flex: 1;
}

