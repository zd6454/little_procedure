.body{
  width: 550rpx;
  height: 330rpx;
  margin:9px 26px 31px 20px
}
.body .scroll{
  width: 281px;
  height: 220rpx;
  display: flex;
  align-items: center;
  margin-top: 1rem;
}
.body .scroll .allimages{
  width: 660rpx;
  height: 220rpx;
  display: flex;
 
 align-items: center;
 margin-top: 1rem;
}


.body .scroll .allimages  .lie{
  display:flex;
  margin-left: 2px;
}


.body .scroll .allimages  .lie .isimage{
width:64px;
height:64px;
background:rgba(255,255,255,1);
opacity:1;
position: relative;
overflow: hidden;
float: left;
align-items:center;
}
.body .scroll .allimages  .lie .setdelete{
  position: fixed;
  margin-left: 50px;
}
.body .scroll .allimages  .lie .setdelete .delete{
width:16px;
height:16px;
background:rgba(0,0,0,0.3);
opacity:1;
border-radius:0px 8px 0px 0px;
position: fixed;
float: 2px;
}
.body .scroll .allimages .last{
  word-break:break-all; 
}
.body .scroll .allimages .addimage{
width:64px;
height:64px;
border:1px solid rgba(229,229,229,1);
opacity:0.4;
border-radius:8px;
}
