}

.wrap_comment{
    width: 270rpx;
  padding-left: 10rpx;
   border-width: 5rpx 10rpx; /*使用padding与预期留白不一致，故使用border*/
  border-style: solid;
  border-color: #f1f1f1;
  line-height: 34rpx;
  font-size: 30rpx;
  background-color: #f1f1f1;
  border-radius: 30rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.release .wrap_comment .img{
  height: 30rpx;
  width: 30rpx;
  flex: 1;
}
 
.release .wrap_comment textarea {
  flex: 8;
  padding-left: 15rpx;
  min-height: 34rpx;
  max-height: 100rpx; /*最多显示三行*/
}
 
.release .text {
  font-size: 40rpx;
  color: #c9c9c9;
}
 
.cancel {
  width: 240rpx;
  height: 64rpx;
  line-height: 64rpx;
  text-align: center;
  color: #6c0;
  margin: 0 3px;
  padding: 0;
}
 
.release .submit {
  width: 70rpx;
  /* height: 64rpx; */
