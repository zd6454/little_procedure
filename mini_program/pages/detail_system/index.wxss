.release {
  /* align-items: center; */
  box-sizing: border-box;
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  /* margin: 0rpx 0rpx 0rpx 15rpx; */
  padding-left: 20rpx;
  padding-top: 20rpx;
  padding-bottom: 10rpx;
  background-color: white;
  font-size: 28rpx;
  z-index: 999;
  display: flex;
  flex-direction: row;
  height: 90rpx;
  overflow: hidden;
}
 
.replyinfo1{ 
  display: flex;
  justify-content: space-between; /*两端对齐*/
  font-size: 35rpx;
}
.replyinfo2{ 
